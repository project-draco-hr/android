{
  mOnExpirationDateInteractionListener=new OnExpirationDateInteractionListener();
  getExpirationDateSwitch().setOnCheckedChangeListener(mOnExpirationDateInteractionListener);
  shareView.findViewById(R.id.shareViaLinkExpirationLabel).setOnClickListener(mOnExpirationDateInteractionListener);
  getExpirationDateValue().setOnClickListener(mOnExpirationDateInteractionListener);
}
