{
  int returnCode=0;
  WebdavClient client=new WebdavClient();
  client.setCredentials(username,password);
  HeadMethod head=new HeadMethod(uri.toString());
  try {
    returnCode=client.executeMethod(head);
  }
 catch (  Exception e) {
    Log.e(TAG,"Error: " + e.getMessage());
  }
  return returnCode;
}
