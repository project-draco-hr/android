{
  return ((BASIC_TOKEN_TYPE.equals(mAuthTokenType) && AuthenticationMethod.BASIC_HTTP_AUTH.equals(authMethod)) || (OAUTH_TOKEN_TYPE.equals(mAuthTokenType) && AuthenticationMethod.BEARER_TOKEN.equals(authMethod)) || (SAML_TOKEN_TYPE.equals(mAuthTokenType) && AuthenticationMethod.SAML_WEB_SSO.equals(authMethod)));
}
