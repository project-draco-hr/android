{
  Vector<OCFile> ret=new Vector<OCFile>();
  if (folder != null) {
    Vector<OCFile> tmp=getFolderContent(folder,onlyOnDevice);
    OCFile current=null;
    for (int i=0; i < tmp.size(); i++) {
      current=tmp.get(i);
      if (current.isImage()) {
        ret.add(current);
      }
    }
  }
  return ret;
}
