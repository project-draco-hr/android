{
  if (DEBUG)   Log.d(TAG,"[dispatchMenuOpened] featureId: " + featureId + ", menu: "+ menu);
  if (featureId == Window.FEATURE_ACTION_BAR || featureId == Window.FEATURE_OPTIONS_PANEL) {
    if (aActionBar != null) {
      aActionBar.dispatchMenuVisibilityChanged(true);
    }
    return true;
  }
  return false;
}
