{
  if (DEBUG)   Log.d(TAG,"[mNativeItemListener.onMenuItemClick] item: " + item);
  final MenuItemImpl sherlockItem=mNativeItemMap.get(item);
  if (sherlockItem != null) {
    sherlockItem.invoke();
  }
 else {
    Log.e(TAG,"Options item \"" + item + "\" not found in mapping");
  }
  return true;
}
