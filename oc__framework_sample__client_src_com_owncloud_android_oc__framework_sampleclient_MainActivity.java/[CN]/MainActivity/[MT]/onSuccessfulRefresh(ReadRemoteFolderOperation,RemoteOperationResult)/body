{
  mFilesAdapter.clear();
  List<RemoteFile> files=result.getData();
  if (files != null) {
    Iterator<RemoteFile> it=files.iterator();
    while (it.hasNext()) {
      mFilesAdapter.add(it.next());
    }
    mFilesAdapter.remove(mFilesAdapter.getItem(0));
  }
  mFilesAdapter.notifyDataSetChanged();
}
