{
  String prefix="";
  String url=((TextView)findViewById(R.id.host_URL)).getText().toString();
  if (mIsSslConn) {
    prefix="https://";
  }
 else {
    prefix="http://";
  }
  if (url.toLowerCase().startsWith("http://") || url.toLowerCase().startsWith("https://")) {
    prefix="";
  }
  continueConnection(prefix);
}
