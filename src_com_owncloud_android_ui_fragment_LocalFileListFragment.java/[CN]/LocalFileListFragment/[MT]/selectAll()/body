{
  int numberOfFiles=mAdapter.getCount();
  for (int i=0; i < numberOfFiles; i++) {
    File file=(File)mAdapter.getItem(i);
    if (file != null) {
      if (file.isDirectory()) {
      }
 else {
        getListView().setItemChecked(i,true);
        mContainerActivity.onFileClick(file);
      }
    }
  }
}
