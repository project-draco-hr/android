{
  OCFileListFragment listOfFiles=getListOfFilesFragment();
  if (mDualPane || getSecondFragment() == null) {
    if (listOfFiles != null) {
      if (mDirectories.getCount() <= 1) {
        finish();
        return;
      }
      popDirname();
      listOfFiles.onBrowseUp();
    }
  }
  if (listOfFiles != null) {
    setFile(listOfFiles.getCurrentFile());
  }
  cleanSecondFragment();
}
