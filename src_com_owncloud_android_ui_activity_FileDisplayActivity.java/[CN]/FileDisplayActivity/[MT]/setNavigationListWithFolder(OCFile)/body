{
  mDirectories.clear();
  OCFile fileIt=file;
  while (fileIt != null && fileIt.getFileName() != OCFile.ROOT_PATH) {
    if (fileIt.isFolder()) {
      mDirectories.add(fileIt.getFileName());
    }
    fileIt=mStorageManager.getFileById(fileIt.getParentId());
  }
  mDirectories.add(OCFile.PATH_SEPARATOR);
}
