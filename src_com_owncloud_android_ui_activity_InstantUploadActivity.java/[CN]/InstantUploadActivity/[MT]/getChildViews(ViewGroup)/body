{
  if (view != null) {
    for (int i=0; i < view.getChildCount(); i++) {
      View cb=view.getChildAt(i);
      if (cb != null && cb instanceof ViewGroup) {
        return getChildViews((ViewGroup)cb);
      }
 else       if (cb instanceof CheckBox) {
        return cb;
      }
    }
  }
  return null;
}
