{
  WebView webview=new WebView(getActivity());
  webview.loadUrl("file:///android_res/raw/" + getResources().getResourceEntryName(R.raw.changelog) + ".html");
  AlertDialog.Builder builder=new AlertDialog.Builder(getActivity());
  Dialog dialog=builder.setView(webview).setIcon(R.drawable.icon).setTitle(R.string.whats_new).setPositiveButton(R.string.common_ok,new DialogInterface.OnClickListener(){
    @Override public void onClick(    DialogInterface dialog,    int which){
      dialog.dismiss();
    }
  }
).create();
  dialog.setCancelable(getArguments().getBoolean(ARG_CANCELABLE));
  return dialog;
}
