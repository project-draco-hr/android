{
  AlertDialog.Builder dialogBuilder=new AlertDialog.Builder(getSherlockActivity());
  int title=getArguments().getInt(ARG_TITLE_ID);
  int message=getArguments().getInt(ARG_MESSAGE_ID);
  dialogBuilder.setIcon(R.drawable.common_error).setTitle(title).setMessage(message).setCancelable(true).setPositiveButton(R.string.common_ok,new DialogInterface.OnClickListener(){
    public void onClick(    DialogInterface dialog,    int whichButton){
      dialog.dismiss();
    }
  }
);
  return dialogBuilder.create();
}
