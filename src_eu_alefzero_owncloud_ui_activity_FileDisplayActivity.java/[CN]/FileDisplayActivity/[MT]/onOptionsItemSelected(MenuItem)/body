{
switch (item.getItemId()) {
case R.id.settingsItem:
{
      Intent i=new Intent(this,Preferences.class);
      startActivity(i);
      break;
    }
case R.id.createDirectoryItem:
{
    showDialog(DIALOG_CREATE_DIR);
    break;
  }
case android.R.id.home:
{
  onBackPressed();
  break;
}
}
return true;
}
