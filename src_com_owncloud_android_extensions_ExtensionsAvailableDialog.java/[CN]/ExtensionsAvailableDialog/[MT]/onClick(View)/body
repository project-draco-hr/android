{
switch (v.getId()) {
case R.id.buttonYes:
{
      Intent i=new Intent(getActivity(),ExtensionsListActivity.class);
      startActivity(i);
      getActivity().finish();
    }
  break;
case R.id.buttonNo:
getActivity().finish();
break;
default :
Log_OC.e("EAD","Button with unknown id clicked " + v.getId());
}
}
