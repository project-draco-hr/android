{
  if (service instanceof FileUploaderBinder) {
    if (mUploaderBinder == null) {
      mUploaderBinder=(FileUploaderBinder)service;
      Log_OC.e(TAG,"UploadListActivity connected to Upload service. component: " + component + " service: "+ service);
    }
 else {
      Log_OC.e(TAG,"mUploaderBinder already set. mUploaderBinder: " + mUploaderBinder + " service:"+ service);
    }
  }
 else {
    Log_OC.e(TAG,"UploadListActivity not connected to Upload service. component: " + component + " service: "+ service);
    return;
  }
}
