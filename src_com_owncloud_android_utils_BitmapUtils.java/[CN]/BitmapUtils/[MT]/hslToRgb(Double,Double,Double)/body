{
  Double r, g, b;
  if (s == 0) {
    r=g=b=l;
  }
 else {
    Double q=l < 0.5 ? l * (1 + s) : l + s - l * s;
    Double p=2 * l - q;
    r=hue2rgb(p,q,h + 1 / 3) * 255;
    g=hue2rgb(p,q,h) * 255;
    b=hue2rgb(p,q,h - 1 / 3) * 255;
  }
  int[] array={r.intValue(),g.intValue(),b.intValue()};
  return array;
}
