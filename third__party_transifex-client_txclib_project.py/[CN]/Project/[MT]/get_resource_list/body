def get_resource_list(self, project=None):
    '\n        Parse config file and return tuples with the following format\n\n        [ (project_slug, resource_slug), (..., ...)]\n        '
    resource_list = []
    for r in self.config.sections():
        if (r == 'main'):
            continue
        (p_slug, r_slug) = r.split('.', 1)
        if (project and (p_slug != project)):
            continue
        resource_list.append(r)
    return resource_list
