{
  double countImages=0;
  double countFiles=0;
  for (  OCFile file : mFiles) {
    if (!file.isFolder()) {
      countFiles++;
      if (file.isImage()) {
        countImages++;
      }
    }
  }
  if ((countImages / countFiles) >= THUMBNAIL_THRESHOLD) {
    return true;
  }
 else {
    return false;
  }
}
