{
  OCFile file=(OCFile)mAdapter.getItem(position);
  if (file != null) {
    if (file.getMimetype().equals("DIR")) {
      mFile=file;
      listDirectory(file);
      mContainerActivity.onDirectoryClick(file);
    }
 else {
      mContainerActivity.onFileClick(file);
    }
  }
 else {
    Log_OC.d(TAG,"Null object in ListAdapter!!");
  }
}
