{
  DownloadFileOperation download=null;
  download=mPendingDownloads.remove(account,file.getRemotePath());
  if (download != null) {
    download.cancel();
  }
 else {
    if (mCurrentDownload.getRemotePath().startsWith(file.getRemotePath()) && account.name.equals(mLastAccount)) {
      mCurrentDownload.cancel();
    }
  }
}
