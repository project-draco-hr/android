{
  Intent intent=new Intent();
switch (position) {
case 0:
    intent.setClass(mContext,FileDisplayActivity.class);
  intent.putExtra("ACCOUNT",AuthUtils.getCurrentOwnCloudAccount(mContext));
break;
case 5:
intent.setClass(mContext,Preferences.class);
break;
default :
intent=null;
}
return intent;
}
