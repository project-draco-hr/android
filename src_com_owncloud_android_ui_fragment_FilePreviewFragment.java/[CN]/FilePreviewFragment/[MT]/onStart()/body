{
  super.onStart();
  if (mFile != null) {
    if (mFile.isAudio()) {
      bindMediaService();
    }
 else     if (mFile.isImage()) {
      BitmapLoader bl=new BitmapLoader(mImagePreview);
      bl.execute(new String[]{mFile.getStoragePath()});
    }
 else     if (mFile.isVideo()) {
      playVideo();
    }
  }
}
