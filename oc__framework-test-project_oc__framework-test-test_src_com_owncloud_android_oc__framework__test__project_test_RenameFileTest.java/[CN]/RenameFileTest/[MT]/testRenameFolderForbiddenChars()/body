{
  RemoteOperationResult result=mActivity.renameFile(mOldFolderName,mOldFolderPath,mNewFolderName + "//",mNewFolderPath + "//" + FileUtils.PATH_SEPARATOR);
  assertTrue(result.getCode() == ResultCode.INVALID_CHARACTER_IN_NAME);
  result=mActivity.renameFile(mOldFolderName,mOldFolderPath,mNewFolderName + "\\",mNewFolderPath + "\\" + FileUtils.PATH_SEPARATOR);
  assertTrue(result.getCode() == ResultCode.INVALID_CHARACTER_IN_NAME);
  result=mActivity.renameFile(mOldFolderName,mOldFolderPath,mNewFolderName + "<",mNewFolderPath + "<" + FileUtils.PATH_SEPARATOR);
  assertTrue(result.getCode() == ResultCode.INVALID_CHARACTER_IN_NAME);
  result=mActivity.renameFile(mOldFolderName,mOldFolderPath,mNewFolderName + ">",mNewFolderPath + ">" + FileUtils.PATH_SEPARATOR);
  assertTrue(result.getCode() == ResultCode.INVALID_CHARACTER_IN_NAME);
  result=mActivity.renameFile(mOldFolderName,mOldFolderPath,mNewFolderName + ":",mNewFolderPath + ":" + FileUtils.PATH_SEPARATOR);
  assertTrue(result.getCode() == ResultCode.INVALID_CHARACTER_IN_NAME);
  result=mActivity.renameFile(mOldFolderName,mOldFolderPath,mNewFolderName + "\"",mNewFolderPath + "\"" + FileUtils.PATH_SEPARATOR);
  assertTrue(result.getCode() == ResultCode.INVALID_CHARACTER_IN_NAME);
  result=mActivity.renameFile(mOldFolderName,mOldFolderPath,mNewFolderName + "|",mNewFolderPath + "|" + FileUtils.PATH_SEPARATOR);
  assertTrue(result.getCode() == ResultCode.INVALID_CHARACTER_IN_NAME);
  result=mActivity.renameFile(mOldFolderName,mOldFolderPath,mNewFolderName + "?",mNewFolderPath + "?" + FileUtils.PATH_SEPARATOR);
  assertTrue(result.getCode() == ResultCode.INVALID_CHARACTER_IN_NAME);
  result=mActivity.renameFile(mOldFolderName,mOldFolderPath,mNewFolderName + "*",mNewFolderPath + "*" + FileUtils.PATH_SEPARATOR);
  assertTrue(result.getCode() == ResultCode.INVALID_CHARACTER_IN_NAME);
}
