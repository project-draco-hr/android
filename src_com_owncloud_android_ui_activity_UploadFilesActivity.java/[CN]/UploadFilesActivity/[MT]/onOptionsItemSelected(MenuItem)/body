{
  boolean retval=true;
switch (item.getItemId()) {
case android.R.id.home:
{
      if (mCurrentDir != null && mCurrentDir.getParentFile() != null) {
        onBackPressed();
      }
      break;
    }
case R.id.actionbar_select_all:
{
    if (selectAllToggled) {
      toggleOffSelectAll();
    }
 else {
      toggleOnSelectAll(item);
    }
    break;
  }
default :
retval=super.onOptionsItemSelected(item);
}
return retval;
}
