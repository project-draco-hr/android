{
  Log_OC.d(TAG,"Uri " + uri);
  Uri newUri=null;
  SQLiteDatabase db=mDbHelper.getWritableDatabase();
  db.beginTransaction();
  try {
    newUri=insert(db,uri,values);
    db.setTransactionSuccessful();
  }
  finally {
    db.endTransaction();
  }
  getContext().getContentResolver().notifyChange(newUri,null);
  return newUri;
}
