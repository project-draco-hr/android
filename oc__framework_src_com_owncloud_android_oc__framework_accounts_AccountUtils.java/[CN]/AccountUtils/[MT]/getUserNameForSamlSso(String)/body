{
  if (authToken != null) {
    String[] cookies=authToken.split(";");
    for (int i=0; i < cookies.length; i++) {
      if (cookies[i].startsWith(KEY_OC_USERNAME_EQUALS)) {
        String value=Uri.decode(cookies[i].substring(KEY_OC_USERNAME_EQUALS.length()));
        return value;
      }
    }
  }
  return "";
}
