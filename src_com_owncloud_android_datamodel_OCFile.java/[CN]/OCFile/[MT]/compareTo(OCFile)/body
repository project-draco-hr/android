{
  if (isFolder() && another.isFolder()) {
    return getRemotePath().toLowerCase().compareTo(another.getRemotePath().toLowerCase());
  }
 else   if (isFolder()) {
    return -1;
  }
 else   if (another.isFolder()) {
    return 1;
  }
  return getRemotePath().toLowerCase().compareTo(another.getRemotePath().toLowerCase());
}
