{
  if (mShowWaitingDialog) {
    mActivity.showLoadingDialog(mActivity.getResources().getString(R.string.wait_for_tmp_copy_from_private_storage));
  }
  CopyAndUploadContentUrisTask copyTask=new CopyAndUploadContentUrisTask((CopyAndUploadContentUrisTask.OnCopyTmpFilesTaskListener)mActivity,mActivity);
  copyTask.execute(CopyAndUploadContentUrisTask.makeParamsToExecute(mAccount,sourceUris,remotePaths,mActivity.getContentResolver()));
}
