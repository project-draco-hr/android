{
  HttpMkCol method=new HttpMkCol(mUri.toString() + path + "/");
  method.setHeader("User-Agent","Android-ownCloud");
  try {
    mHttpClient.execute(mTargetHost,method,mHttpContext);
    Log.i(TAG,"Creating dir completed");
  }
 catch (  final Exception e) {
    e.printStackTrace();
    return false;
  }
  return true;
}
