{
synchronized (thumbnailDiskCacheLock) {
    while (mThumbnailCacheStarting) {
      try {
        thumbnailDiskCacheLock.wait();
      }
 catch (      InterruptedException e) {
      }
    }
    if (mThumbnailCache != null) {
      return mThumbnailCache.getBitmap(key);
    }
  }
  return null;
}
