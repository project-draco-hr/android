{
synchronized (thumbnailDiskCacheLock) {
    try {
      mThumbnailCache=new DiskLruImageCache(mContext,"thumbnailCache",DISK_CACHE_SIZE,mCompressFormat,mCompressQuality);
    }
 catch (    Exception e) {
      Log_OC.d(TAG,"Thumbnail cache could not be opened ",e);
      mThumbnailCache=null;
    }
    mThumbnailCacheStarting=false;
    thumbnailDiskCacheLock.notifyAll();
  }
  return null;
}
