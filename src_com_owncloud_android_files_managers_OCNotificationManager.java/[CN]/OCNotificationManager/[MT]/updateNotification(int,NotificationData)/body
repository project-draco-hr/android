{
  if (!mNotificationMap.containsKey(notification_id)) {
    return false;
  }
  NotificationTypePair pair=mNotificationMap.get(notification_id);
switch (pair.mType) {
case NOTIFICATION_PROGRESS:
    pair.mNotificaiton.contentView.setProgressBar(R.id.status_text,100,data.getPercent(),false);
  return true;
case NOTIFICATION_SIMPLE:
pair.mNotificaiton=new Notification(R.drawable.icon,data.getText(),System.currentTimeMillis());
mNM.notify(notification_id,pair.mNotificaiton);
return true;
default :
return false;
}
}
