{
  OCFile file=mFile;
  if (file != null) {
    if (file.isFolder()) {
      return file;
    }
 else     if (getStorageManager() != null) {
      String parentPath=file.getRemotePath().substring(0,file.getRemotePath().lastIndexOf(file.getFileName()));
      return getStorageManager().getFileByPath(parentPath);
    }
  }
  return null;
}
