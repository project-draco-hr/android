{
  Cursor c=mDB.query(TABLE_SESSIONS,null,null,null,null,null,null);
  Vector<OwnCloudSession> v=new Vector<OwnCloudSession>();
  if (!c.moveToFirst()) {
    return v;
  }
  while (!c.isAfterLast()) {
    v.add(new OwnCloudSession(c.getString(c.getColumnIndex("sessionName")),c.getString(c.getColumnIndex("sessionUrl")),c.getInt(c.getColumnIndex("_id"))));
    c.moveToNext();
  }
  c.close();
  return v;
}
