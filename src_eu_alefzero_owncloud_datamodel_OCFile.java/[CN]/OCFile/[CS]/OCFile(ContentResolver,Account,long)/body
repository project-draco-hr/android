{
  this.contentResolver=resolver;
  this.account=account;
  Cursor c=this.contentResolver.query(ProviderTableMeta.CONTENT_URI_FILE,null,ProviderTableMeta.FILE_ACCOUNT_OWNER + "=? AND " + ProviderTableMeta._ID+ "=?",new String[]{this.account.name,String.valueOf(id)},null);
  if (c.moveToFirst())   setFileData(c);
}
