{
  if (isDirectory() && another.isDirectory()) {
    return getRemotePath().toLowerCase().compareTo(another.getRemotePath().toLowerCase());
  }
 else   if (isDirectory()) {
    return -1;
  }
 else   if (another.isDirectory()) {
    return 1;
  }
  return getRemotePath().toLowerCase().compareTo(another.getRemotePath().toLowerCase());
}
