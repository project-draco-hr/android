{
  OCFile file=new OCFile(we.path());
  file.setCreationTimestamp(we.createTimestamp());
  file.setFileLength(we.contentLength());
  String filename=file.getFileName();
  String mimeType=MimeTypeMap.getSingleton().getMimeTypeFromExtension(filename.substring(filename.lastIndexOf('.') + 1));
  if (mimeType == null)   file.setMimetype(we.contentType());
 else   file.setMimetype(mimeType);
  file.setModificationTimestamp(we.modifiedTimesamp());
  file.setLastSyncDate(mCurrentSyncTime);
  return file;
}
