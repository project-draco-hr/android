{
  String[] whereArgs=new String[2];
  whereArgs[0]=String.valueOf(UploadStatus.UPLOAD_SUCCEEDED.value);
  whereArgs[1]=String.valueOf(UploadStatus.UPLOAD_FAILED.value);
  long result=getDB().delete(ProviderTableMeta.CONTENT_URI_UPLOADS,ProviderTableMeta.UPLOADS_STATUS + "=? OR " + ProviderTableMeta.UPLOADS_STATUS+ "=?",whereArgs);
  Log_OC.d(TAG,"delete all finished uploads");
  if (result > 0) {
    notifyObserversNow();
  }
  return result;
}
