{
  return getUploads(ProviderTableMeta.UPLOADS_STATUS + "==" + UploadStatus.UPLOAD_IN_PROGRESS.value+ " OR "+ ProviderTableMeta.UPLOADS_STATUS+ "=="+ UploadStatus.UPLOAD_LATER.value+ " OR "+ ProviderTableMeta.UPLOADS_STATUS+ "=="+ UploadStatus.UPLOAD_FAILED_RETRY.value+ " OR "+ ProviderTableMeta.UPLOADS_STATUS+ "=="+ UploadStatus.UPLOAD_PAUSED.value,null);
}
