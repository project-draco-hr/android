{
  SharedPreferences.Editor appPrefsE=PreferenceManager.getDefaultSharedPreferences(getApplicationContext()).edit();
  SharedPreferences appPrefs=PreferenceManager.getDefaultSharedPreferences(getApplicationContext());
  boolean state=appPrefs.getBoolean(PREFERENCE_SET_PASSCODE,false);
  appPrefsE.putBoolean(PREFERENCE_SET_PASSCODE,!state);
  appPrefsE.commit();
  finish();
}
